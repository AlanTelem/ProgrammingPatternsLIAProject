<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="144.0" prefWidth="578.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="patternsproject.AddFriendsController">
   <children>
      <Label fx:id="addFriendLbl" layoutX="14.0" layoutY="23.0" text="Insert the name of the user you would like to friend:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="addFriendTextField" layoutX="14.0" layoutY="65.0" prefHeight="25.0" prefWidth="549.0" />
      <Button fx:id="addFriendBtn" layoutX="440.0" layoutY="109.0" mnemonicParsing="false" onAction="#addFriendClick" prefHeight="25.0" prefWidth="123.0" text="Add Friend" />
   </children>
</AnchorPane>
